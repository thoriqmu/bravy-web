generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Event {
  id          String   @id @default(cuid())
  title       String
  startAt     DateTime
  location    String
  brochureUrl String?
  photos      Json?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@index([startAt])
}

model FeaResult {
  id           String   @id @default(cuid())
  userId       String?
  durationSec  Int
  confidence   Int
  aus          Json
  createdAt    DateTime @default(now())
}
